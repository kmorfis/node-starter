{
  "rootDir": "../../",
  "testRegex": "\\.spec\\.ts$",
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "transform": {
    ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "restoreMocks": true,
  "reporters": [ "default", ["jest-junit", {
    "outputDirectory": "reports/test",
    "outputName": "jest.xml"
     
  }] ]
}
